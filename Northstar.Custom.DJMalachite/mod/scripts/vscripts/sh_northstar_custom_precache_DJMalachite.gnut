global function NorthstarCustomPrecacheDJMalachite

void function NorthstarCustomPrecacheDJMalachite()
{
	
        PrecacheWeapon( "mp_titanweapon_meteor_scuffed" )
        PrecacheWeapon( "mp_weapon_sniper2" )
	
	// create kunai damage source so game won't crash when we hit smth with it
